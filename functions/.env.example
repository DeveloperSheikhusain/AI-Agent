# Copy this file to .env and fill in your actual values
# Note: .env file should be in .gitignore

# AWS Bedrock Configuration
AWS_ACCESS_KEY=
AWS_SECRET_KEY=
AWS_REGION=us-east-1
AWS_BEDROCK_AGENT_ID=
AWS_BEDROCK_AGENT_ALIAS_ID=

# Google Cloud Configuration (for Translation API)
GOOGLE_CLOUD_PROJECT_ID=
# Note: GOOGLE_APPLICATION_CREDENTIALS should point to service account JSON file

# Google Cloud BigQuery Billing Configuration (for Cost Retrieval)
GCP_BILLING_DATASET=ss_gcp_billing
GCP_BILLING_TABLE_PREFIX=gcp_billing_export_v1

# Facebook Messenger Configuration
FACEBOOK_PAGE_ACCESS_TOKEN=
FACEBOOK_VERIFY_TOKEN=
FACEBOOK_APP_ID=
FACEBOOK_APP_SECRET=

# Instagram Configuration (uses same token as Facebook for most cases)
INSTAGRAM_PAGE_ACCESS_TOKEN=
INSTAGRAM_VERIFY_TOKEN=

# WhatsApp Business Configuration
WHATSAPP_PHONE_NUMBER_ID=
WHATSAPP_ACCESS_TOKEN=
WHATSAPP_VERIFY_TOKEN=
